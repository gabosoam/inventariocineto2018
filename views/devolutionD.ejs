    <div class="modal fade" id="myModalDev" role="dialog">
        <div class="modal-dialog">

            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Modal Header</h4>
                </div>
                <div class="modal-body">
                    <div class="form-group">

                        <label for="code">Obervación: (opcional)</label>
                        <textarea autofocus style="resize: none" class="form-control" name="observation" id="observation" rows="2" placeholder="Ingrese una observación"></textarea><br>


                        <div class="form-group">
                            <label for="pwd">Almacén:</label>
                            <input type="text" class="form-control" validationMessage="Selecciona un almacén" name="location" required id="location"
                                style="width: 100%">
                        </div>

                        
                        <input autofocus type="hidden" class="form-control" required id="code2" name="code" style="width: 100%" placeholder="Ingresar número de serie">


                        <input id="voucher" type="hidden" value="<%=bill%>">
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary" onclick="saveDevolution()">Guardar</button>
                    <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>
                </div>
            </div>

        </div>
    </div>
    <div class="modal fade" id="myModal2" role="dialog">
        <div class="modal-dialog modal-lg">

            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Reporte</h4>
                </div>
                <div class="modal-body">
                    <button onclick="print()" class="btn btn-primary">Imprimir</button>
                    <div id="print" style="min-width: 210mm">

                        <table style="width: 100%">
                            <tr>
                                <td></td>
                                <td>
                                    <center>
                                        <img class="" src="../image/logo.svg" width="280px" />




                                    </center>
                                </td>
                                <td style="text-align: right">
                                    <h5>DEV-
                                        <%=data.id%>
                                    </h5>
                                </td>
                            </tr>
                        </table>


                        <hr>
                        <center><strong style="font-size: 18px">Acta de retiro</strong></center>
                        <table style="width: 100%; font-size: 12px">
                            <tr>
                                <td>

                                    <address>
                                        <br>
                                        <strong><%=data.name%></strong><br>
                                        <%=data.address%><br>
                                            <%=data.email%>,<br>
                                                <%=data.phone%>
                                    </address>
                                </td>
                                <td style="text-align: right">

                                    <address>
                                        <strong>Fecha:</strong>
                                        <%=data.date%> <br>
                                            <strong>Referencia:</strong>
                                            <%=data.reference%>

                                    </address>
                                </td>
                            </tr>

                        </table>
                        <hr>
                        <table id="Voucher" class="table ">
                            <thead style="font-size: 13px">
                                <tr>
                                    <th style="text-align: center">Item</th>
                                    <th style="text-align: center">Descripción</th>
                                    <th style="text-align: center">Código</th>
                                    <th style="text-align: center">Serie</th>
                                    <th style="text-align: center">Cant.</th>
                                    <th style="text-align: center">Observación</th>
                                </tr>
                            </thead>
                            <tbody style="font-size: 11px">
                                <tr w3-repeat="vouchers">
                                    <td style="text-align: center">{{num}}</td>
                                    <td>{{description}}</td>
                                    <td style="text-align: center">{{code}}</td>
                                    <td style="text-align: center">{{barcode}}</td>
                                    <td style="text-align: center">{{count}}</td>
                                    <td style="text-align: center"><input style="border: 0; width: 100%" type="text" value="{{observation}}"></td>
                                </tr>


                            </tbody>
                        </table>
                        <hr>

                        <div class="well m-t"><strong>Comentario</strong>
                            <input class="form-control" type="text">
                        </div>



                        <table style="width: 100%; border: 1; font-size: 12px ">
                            <tr>
                                <td>
                                    <center>
                                        <p>
                                            REPRESENTANTE CLIENTE <br><br><br><br><br> _______________________________<br><br>
                                            <%=data.name%>
                                        </p>
                                    </center>

                                </td>
                                <td>
                                    <center>
                                        <p>
                                            REPRESENTANTE CINETO <br><br><br><br><br> _______________________________<br><br>
                                            <%=data.user%>
                                        </p>
                                    </center>


                                </td>
                            </tr>
                        </table>









                    </div>
                </div>
            </div>
        </div>


    </div>
    <% if (user) { %>
